# pre-requisite packages
1. sudo yum install openssl-devel


# Install specific version of ansible in a virtualenv

1. sudo pip install --upgrade pip virtualenv virtualenvwrapper
2. virtualenv ansible1.9 
3. source ansible1.9/bin/activate
4. pip install ansible==1.9.4


# Run ansible playbook locally

1. ansible-playbook -i "localhost," -c local playbook.yml
